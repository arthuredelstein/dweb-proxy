{

  "manifest_version": 2,
  "name": "proxy-dweb",
  "description": "Connects to a proxy to load dweb protocols",
  "version": "0.1",

  "icons": {
    "48": "icon.svg",
    "96": "icon.svg"
  },

  "background": {
    "page": "background.html"
  },

  "permissions": ["proxy", "webNavigation", "webRequest", "webRequestBlocking", "<all_urls>", "storage"],

  "browser_action": {
    "default_title": "Dweb Express",
    "default_icon": "icon.svg",
    "default_popup": "popup.html"
  }

}
