{

  "manifest_version": 2,
  "name": "dweb-proxy",
  "description": "Connects to a proxy to load dweb protocols",
  "version": "0.0.10",

  "icons": {
    "48": "icon.svg",
    "96": "icon.svg"
  },

  "background": {
    "scripts": ["background.js"]
  },

  "permissions": ["proxy", "webNavigation", "webRequest", "webRequestBlocking", "storage", "<all_urls>"],

  "browser_action": {
    "default_title": "Dweb Express",
    "default_icon": "icon.svg",
    "default_popup": "popup.html"
  }

}
